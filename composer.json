{
    "name": "webuni/doctrine-extensions",
    "description": "Doctrine extensions",
    "type": "library",
    "require": {
        "php": "^7.2",
        "doctrine/orm": "^2.6.3",
        "doctrine/inflector": "^1.0|^2.0"
    },
    "require-dev": {
        "bentools/doctrine-ulid": "^1.0",
        "ramsey/uuid-doctrine": "^1.0",
        "symfony/uid": "^5.1",
        "symfony/http-kernel": "^3.4|^4.2|^5.0",
        "symfony/var-dumper": "^3.4|^4.2|^5.0",
        "symfony/dependency-injection": "^3.4|^4.2|^5.0",
        "twig/twig": "^1.29|^2.0|^3.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Martin Haso≈à",
            "email": "martin.hason@gmail.com"
        },
        {
            "name": "Webuni s.r.o.",
            "homepage": "https://www.webuni.cz"
        }
    ],
    "autoload": {
        "psr-4": {
            "Webuni\\DoctrineExtensions\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Webuni\\DoctrineExtensions\\Tests\\": "tests"
        }
    },
    "scripts": {
        "cs": "vendor/bin/php-cs-fixer fix --ansi --allow-risky=yes",
        "cs-check": "vendor/bin/php-cs-fixer fix --dry-run --diff --ansi --allow-risky=yes",
        "analyse": "vendor/bin/phpstan analyse -l 7 -c phpstan.neon --no-progress --ansi src",
        "tests": "vendor/bin/phpunit --colors=always"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
